{
  "env": {
    "browser": true,
    "es2021": true,
    "mocha": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12, //TODO replace with allowed value
    "sourceType": "module"
  },
  "plugins": [
    "mocha"
  ],
  "rules": {
    "mocha/no-skipped-tests": "error",
    "mocha/no-exclusive-tests": "error"
  }
}
